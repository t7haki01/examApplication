{% extends 'student/student_template.html.twig' %}

{% block css %}{% endblock %}

{% block body %}
    <h1>Here comes student main </h1>
    <div><a href="{{ path('exam_main', {'studentId':studentId}) }}">
            <button>Take Exam</button>
        </a></div>
    <p>Exam Result</p>
    <table>
    {% for data in examResult%}
        <tr><td><table class="table">
            <tr>
                <td>Exam Title</td>
                <td>{{ data.exam.examTitle }}</td>
            </tr>
            <tr>
                <td>Grade</td>
                <td>{{ data.score }}</td>
            </tr>
                    <tr><td colspan="2">
                            <a href="{{ path('student_check_result', {'examId':data.exam.id, 'studentId':studentId}) }}"><button>
                                    Click Here to Check
                                </button></a>
                        </td></tr>
                </table></td></tr>
    {% endfor %}
    </table>
{% endblock %}